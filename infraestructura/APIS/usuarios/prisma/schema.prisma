
generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"] 
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id Int @id @default(autoincrement())
  email String @unique
  password String
  person Persons[] 
} 

model Persons {
  id Int @id @default(autoincrement())
  name String?
  lastName String
  number String?
  published Boolean @default(false)
  user Users @relation(fields: [userID], references: [id])
  userID Int
}